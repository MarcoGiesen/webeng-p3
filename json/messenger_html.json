{
  "main": {
    "tag": "div",
    "class": "main",
    "inner": [
      {
        "tag": "div",
        "class": "header"
      },
      {
        "tag": "div",
        "class": "chat-overview"
      },
      {
        "tag": "div",
        "class": "messages"
      },
      {
        "tag": "div",
        "class": "bottom"
      }
    ]
  },
  "chat": {
    "tag": "div",
    "class": "chat",
    "inner": [
      {
        "tag": "div",
        "class": "name",
        "inner": "%name%",
        "onclick": "%onclick%"
      }
    ]
  },
  "message": {
    "tag": "div",
    "class": "message",
    "inner": [
      {
        "tag": "div",
        "class": "name",
        "inner": "%name%"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": "%text%"
      }
    ]
  },
  "inputMessage": {
    "tag": "div",
    "class": "new_message",
    "inner": [
      {
        "tag": "div",
        "class": "name"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "class": "message-input",
              "placeholder": "Enter message here..."
            },
            {
              "tag": "input",
              "class": "button",
              "type": "submit",
              "value": "Send"
            }
          ],
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  },
  "inputChat": {
    "tag": "div",
    "class": "new_chat",
    "inner": [
      {
        "tag": "div",
        "class": "names"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "class": "chat-input",
              "placeholder": "Enter one or more names"
            },
            {
              "tag": "input",
              "class": "button",
              "type": "submit",
              "value": "Anlegen"
            }
          ],
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  },
  "userInfo": {
    "tag": "div",
    "class": "userActions",
    "inner": "%username%"
  }
}
